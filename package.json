{
  "name": "block-drop",
  "version": "0.0.1",
  "description": "Block dropping game",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:angular && npm run build:complete && npm run build:engine && npm run build:react",
    "build:angular": "cross-env NODE_ENV=production STAND_ALONE=true webpack --config ./webpack/webpack.angular.config.js",
    "build:complete": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.complete.config.js",
    "build:engine": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.engine.config.js",
    "build:react": "cross-env NODE_ENV=production STAND_ALONE=true webpack --config ./webpack/webpack.react.config.js",
    "clean": "rimraf ./coverage && rimraf ./dist && rimraf ./lib",
    "dev": "cross-env NODE_ENV=debug webpack-dev-server",
    "lint": "tslint ./src/**/*.ts",
    "test": "npm run lint && karma start --singleRun true",
    "test:watch": "karma start --singleRun false --restartOnFileChange true",
    "tsc": "tsc -p ./tsconfig.build.json",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennett000/block-drop.git"
  },
  "keywords": [
    "game",
    "block",
    "drop",
    "gravity",
    "clear",
    "lines"
  ],
  "author": "Michael J. Bennett",
  "contributors": [],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bennett000/block-drop/issues"
  },
  "homepage": "https://github.com/bennett000/block-drop#readme",
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "awesome-typescript-loader": "^2.0.0-rc.18",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "connect": "~2.16.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-node": "~1.14.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "ng2-redux": "^3.3.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.2",
    "rxjs": "^5.0.0-beta.6",
    "seedrandom": "^2.4.2",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.11.0",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.0",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-split-by-path": "^0.1.0",
    "zone.js": "^0.6.12"
  }
}
